echo Rendering replica wheel m94.stl.
openscad -o m94.stl -D 'REPLICA = true' m94.scad
for wheel in {0..25}
do
    echo Rendering wheel $wheel.
    openscad -o m94-${wheel}.stl -D 'REPLICA = false'  -D "WHEEL = ${wheel}" m94.scad
done
openscad -o 4x4-1.stl -D 'START = 1' 4x4.scad
openscad -o 4x4-2.stl -D 'START = 17' 4x4.scad
zip all-wheels m94-*.stl 4x4-*.stl
